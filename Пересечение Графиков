import matplotlib.pyplot as plt
import numpy as np


def plotCrossing(fX1, fX2):
    x = np.linspace(-10, 10, 100)
    y1 = eval(fX1)
    y2 = eval(fX2)
    plt.figure()
    plt.plot(x, y1, label='График Функции №1')
    plt.plot(x, y2, label='График Функции №2')
    plt.title('Пересечение графиков')
    plt.xlabel('x')
    plt.ylabel('y')
    plt.grid(True)
    plt.show()
